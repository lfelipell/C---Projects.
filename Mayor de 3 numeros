#include "stdafx.h"
#include "conio.h"
#include "stdio.h"


void main()
{int a,b,c;
printf("Ingrese 3 numeros: ");
scanf("%d",&a);
scanf("%d",&b);
scanf("%d",&c);
if(a>b)
  if(a>c)
    printf("El mayor es: %d",a);
  else
	  printf("El mayor es: %d",c);
else
  if(b>c)
	  printf("El mayor es: %d",b);
  else
	  printf("El mayor es: %d",c);
getch();
}
