#include "stdafx.h"
#include "stdio.h"
#include "stdlib.h"
#include "conio.h"

void main()
{
  float  monto,tp,dolares;
	    printf("Ingrese monto: ");
	    scanf("%f",&monto);
		printf("Ingrese tipo de cambio: ");
		scanf("%f",&tp);
     if(monto>=0 && tp>0)
	   {dolares=monto/tp;
	    printf("\n En dolares es: %f",dolares);
	   }
	 else
		 printf("\n ERROR");
	 getch();
}
