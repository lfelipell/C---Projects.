#include "stdafx.h"
#include "iostream"
#include "conio.h"
using namespace std;

int main(){
  int n,nro,i,cont=0;
	cout << "introduzca cantidad de numeros : ";
	cin >> n;
    for(i=0;i<n;i++)
	{
		cout << "introducir numero: ";
		cin >> nro;
		if(nro % 2!= 0){
			cont++;}
	}

	
	
    cout << "\nLos numeros impares igresados son: "<< cont;

	
	getch();
	return(0);
}

